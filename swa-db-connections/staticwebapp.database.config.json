{
  "$schema": "https://github.com/Azure/data-api-builder/releases/latest/download/dab.draft.schema.json",
  "data-source": {
    "database-type": "mssql",
    "options": {
      "set-session-context": false
    },
    "connection-string": "Server=tcp:delta-team.database.windows.net,1433;Initial Catalog=class_registration;Persist Security Info=False;User ID=delta_team;Password=classREGISTRATION!;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },
  "runtime": {
    "rest": {
      "enabled": true,
      "path": "/rest"
    },
    "graphql": {
      "allow-introspection": true,
      "enabled": true,
      "path": "/graphql"
    },
    "host": {
      "mode": "production",
      "cors": {
        "origins": [
          "http://localhost:4200"
        ],
        "allow-credentials": false
      },
      "authentication": {
        "provider": "StaticWebApps"
      }
    }
  },
  "entities": {
    "department": {
      "source": "department",
      "permissions": [
        {
          "actions": [
            "*"
          ],
          "role": "anonymous"
        }
      ]
    },
    "class_data": {
      "source": "class_data",
      "permissions": [
        {
          "actions": [
            "*"
          ],
          "role": "anonymous"
        }
      ]
    },
    "classes": {
      "source": "classes",
      "permissions": [
        {
          "actions": [
            "*"
          ],
          "role": "anonymous"
        }
      ]
    },
  "courses": {
    "source": "courses",
    "permissions": [
      {
        "actions": [
          "*"
        ],
        "role": "anonymous"
      }
    ]
  }
}
}